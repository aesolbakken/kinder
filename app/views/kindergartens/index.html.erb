<h1>Listing kindergartens</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @kindergartens.each do |kindergarten| %>
  <tr>
    <td><%= kindergarten.name %></td>
    <td><%= kindergarten.email %></td>
    <td><%= kindergarten.phone %></td>
    <td><%= link_to 'Show', kindergarten %></td>
    <td><%= link_to 'Edit', edit_kindergarten_path(kindergarten) %></td>
    <td><%= link_to 'Destroy', kindergarten, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Kindergarten', new_kindergarten_path %>
